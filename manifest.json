{
  "manifest_version": 2,

  "name": "CookiePaste",
  "description": "Copy values of cookies from one domain to another",
  "version": "0.1",

  "icons": { "16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon128.png" },

  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "cookies",
    "http://*/*", "https://*/*"
  ]
}
