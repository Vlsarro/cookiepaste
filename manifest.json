{
  "manifest_version": 2,

  "name": "CookiePaste",
  "description": "Copy values of cookies from one domain to another",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "http://*/*", "https://*/*"
  ]
}
